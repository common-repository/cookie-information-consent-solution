{
  "name": "piotrpress/logger",
  "type": "library",
  "license": "GPL-3.0",
  "description": "This library is compatible with PSR-3 file logger implementation.",
  "homepage": "https://github.com/PiotrPress/logger",
  "authors": [
    {
      "name": "Piotr Niewiadomski",
      "homepage": "https://piotr.press"
    }
  ],
  "keywords": [
    "logger", "logging", "logs", "php", "psr-3", "psr3", "php logging library", "file logger", "logging to file", "error logger"
  ],
  "require": {
    "php": ">=7.4",
    "psr/log": "^1.1.4",
    "piotrpress/templater": "^1.2.0"
  },
  "autoload": {
    "psr-4": {
      "PiotrPress\\": "src/",
      "PiotrPress\\Logger\\": "src/",
      "PiotrPress\\Logger\\Formatter\\": "src/Formatter/",
      "PiotrPress\\Logger\\Handler\\": "src/Handler/"
    },
    "classmap": [ "src" ]
  }
}